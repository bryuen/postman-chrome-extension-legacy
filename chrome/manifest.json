{
    "manifest_version": 2,
    "name":"__MSG_name__",
    "version":"0.8.4.20",
    "icons":{
        "16":"icon_16.png",
        "32":"icon_32.png",
        "48":"icon_48.png",
        "128":"icon_128.png"
    },

    "browser_action": {
      "default_icon": {
        "16":"icon_16.png",
        "32":"icon_32.png",
        "48":"icon_48.png",
        "128":"icon_128.png"
      },
      "default_title": "Postman - REST Client"
    },

    "background": {
      "scripts": ["js/background.js"]
    },

    "default_locale":"en",
    "permissions":["cookies", "storage", "<all_urls>"],
    "content_security_policy": "default-src 'self'; connect-src *; script-src 'self'; style-src * blob: data: filesystem: 'unsafe-inline'; img-src *; frame-src * blob: data: filesystem:; font-src * blob: data: filesystem:; media-src *;",
    "offline_enabled":true
}
